{
  "name": "graank-tool",
  "version": "1.0.0",
  "description": "a software for mining gradual patterns",
  "main": "main.js",
  "bin": "cli.js",
  "scripts": {
    "start": "electron .",
    "dev": "electron . --debug",
    "package": "npm-run-all package:*",
    "installer": "npm-run-all installer:*",
    "package:mac": "electron-packager . --overwrite --platform=darwin --arch=x64 --out=dist-builds --icon=assets/app-icon/mac/app.icns --prune=true",
    "package:win": "electron-packager . --overwrite --platform=win32 --arch=ia32 --out=dist-builds --icon=assets/app-icon/win/app.ico",
    "package:linux": "electron-packager . --overwrite --platform=linux --arch=x64 --prune=true --out=dist-builds",
    "installer:deb64": "electron-installer-debian --overwrite --src dist-builds/graank-tool-linux-x64/ --dest dist/installers/ --arch amd64",
    "installer:win32": "electron-installer-windows --overwrite --src dist-builds/graank-tool-win32-ia32/ --dest dist/installers/",
    "installer:dmg": "electron-installer-dmg --overwrite dist-builds/graank-tool-darwin-x64/graank-tool.app dist/installers/graank-tool",
    "prepack": "check-for-leaks",
    "prepush": "check-for-leaks"
  },
  "repository": "https://github.com/owuordickson/graank-tool",
  "keywords": [
    "gradual",
    "patterns"
  ],
  "author": "Dickson Owuor",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/owuordickson/graank-tool/issues"
  },
  "homepage": "https://github.com/owuordickson/graank-tool#readme",
  "devDependencies": {
    "check-for-leaks": "^1.2.0",
    "devtron": "^1.3.0",
    "electron": "^4.0.6",
    "electron-installer-debian": "^1.1.1",
    "electron-installer-dmg": "^2.0.0",
    "electron-installer-windows": "^1.1.1",
    "electron-packager": "^13.1.0",
    "npm-run-all": "^4.0.2"
  },
  "dependencies": {
    "bootstrap": "^4.1.3",
    "csvtojson": "^2.0.8",
    "electron-log": "^2.2.14",
    "electron-settings": "^3.0.7",
    "electron-shortcut-normalizer": "^1.0.0",
    "electron-tooltip": "^1.1.5",
    "glob": "^7.1.0",
    "jquery": "^3.3.1",
    "mime": "^2.4.0",
    "mmmagic": "^0.5.2",
    "popper.js": "^1.14.4",
    "python-shell": "^1.0.7",
    "update-electron-app": "^1.1.1"
  }
}
